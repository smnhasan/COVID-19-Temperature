keywords = {Air Pollutants / analysis*,Air Pollution / analysis*,Betacoronavirus,COVID-19,China,Cities,Coronavirus Infections*,Humans,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,PMC7832158,Pandemics*,Particulate Matter / analysis,Pneumonia,PubMed Abstract,SARS-CoV-2,Ting Xue,Viral*,Xiaoyu Jin,Zhenbo Zhang,doi:10.1016/j.scitotenv.2020.140244,pmid:32592975},
