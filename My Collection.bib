issn = {22549625},
journal = {European Journal of Investigation in Health, Psychology and Education 2021, Vol. 11, Pages 358-371},
keywords = {19,COVID,E),Exercise Self,International Physical Activity Questionnaire (IPAQ),Regulation Questionnaire (SRQ,pandemic,physical inactivity,university students},
month = {apr},
number = {2},
pages = {358--371},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Exploring Two Pandemics in Academic Arena: Physical Activity and Sedentary Behaviors Profile of University Students in Bangladesh}},
url = {https://www.mdpi.com/2254-9625/11/2/27/htm https://www.mdpi.com/2254-9625/11/2/27},
volume = {11},
year = {2021}
