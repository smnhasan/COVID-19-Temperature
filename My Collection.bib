keywords = {Arnab Chanda,Betacoronavirus,COVID-19,Coronavirus Infections / epidemiology*,Forecasting,Gourav Singh Raghuwanshi,Humans,Humidity,India,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,PMC7194548,Pandemics,Pneumonia,PubMed Abstract,SARS-CoV-2,Sonal Gupta,Temperature,United States / epidemiology,Viral / epidemiology*,Weather*,doi:10.1016/j.scitotenv.2020.138860,pmid:32334160},
